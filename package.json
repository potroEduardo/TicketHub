{
  "name": "Ticket Hub",
  "version": "1.0.0",
  "description": "Sistema de Gesti√≥n de Eventos y Venta de Boletos (TicketHub) - Proyecto BDA.",
  "main": "app.js",
  "directories": {
    "test": "src/test"
  },
  "scripts": {
    "start": "babel-watch src/app.js",
    "heroku-postbuild": "npm run build",
    "build": "babel src -d dist --copy-files --no-comments && npm run remove-cache",
    "serve": "node dist/app.js",
    "remove-cache": "rm -rf dist/cache/* && touch dist/cache/cache.txt"
  },
  "babel": {
    "presets": [
      "env"
    ]
  },
  "repository": {
    "type": "git",
