  },
  "homepage": "https://github.com/potroEduardo/TicketHub#readme",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.2",
    "compression": "^1.7.2",
    "connect-mongo": "^2.0.1",
    "dotenv": "^5.0.1",
    "ejs": "^2.5.8",
    "express": "^4.16.3",
    "express-http-to-https": "^1.1.4",
